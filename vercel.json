{
    "functions": {
        "backend/src/**/*.js": {
            "runtime": "@vercel/node"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/backend/src/server.js"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/admin/dist/index.html"
        }
    ]
}